generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Alunos {
  id_Aluno  Int       @id @default(autoincrement())
  name      String
  course    Courses[]
}

model Courses {
  id_Course Int    @id @unique @default(autoincrement())
  course    String
  id_Aluno  Int
  aluno     Alunos @relation(fields: [id_Aluno], references: [id_Aluno])
}
